        </mxCell>
        <mxCell id="7mCWovDAdpfkG4Z2055h-188" value="&lt;p style=&quot;line-height: 100%&quot;&gt;Методы Верт. марк.&lt;/p&gt;" style="swimlane;rounded=0;strokeWidth=1;gradientColor=#ffffff;align=center;html=1;startSize=21;" parent="7mCWovDAdpfkG4Z2055h-186" vertex="1" collapsed="1">
