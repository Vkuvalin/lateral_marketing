        <mxCell id="PM2-UTOyU-0HjiK0SGNb-58" value="Оценка. (Установ. связи)" style="swimlane;rounded=0;strokeWidth=1;gradientColor=#ffffff;html=1;snapToPoint=0;startSize=25;connectable=0;" parent="7mCWovDAdpfkG4Z2055h-164" vertex="1" collapsed="1">
