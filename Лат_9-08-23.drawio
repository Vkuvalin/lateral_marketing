        <mxCell id="PM2-UTOyU-0HjiK0SGNb-186" value="Гиперболизация" style="swimlane;rounded=0;strokeWidth=1;gradientColor=#ffffff;html=1;snapToPoint=0;startSize=25;connectable=0;" parent="1" vertex="1" visible="0">
          <mxGeometry x="-776" y="2310" width="690" height="200" as="geometry">
