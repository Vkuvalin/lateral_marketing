        <mxCell id="PM2-UTOyU-0HjiK0SGNb-29" value="&lt;p style=&quot;line-height: 100%&quot;&gt;Фокус&lt;/p&gt;" style="swimlane;rounded=0;strokeWidth=1;align=center;html=1;startSize=21;gradientColor=none;swimlaneFillColor=none;" parent="1" vertex="1" visible="0">
